  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.config.js"
    },
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    }
  },
  "nyc": {
    "include": [
      "src/main/**/*.ts",
      "src/common/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "test/**",
      "coverage/**",
      "src/renderer/*",
      "src/main/index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "build": {
    "asar": true,
    "directories": {
      "buildResources": "static/assets/images/icons/"
    }
  }
}
