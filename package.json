{
  "name": "Rooster-XSync",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Rooster-X Desktop Sync Application, used to sync Excel files to Rooster-X.",
  "author": "Rooster-X <rooster-x@rooster-x.com>",
  "scripts": {
    "fix": "tslint --fix 'src/**/*.ts'",
    "dev": "yarn fix && yarn test-unit && electron-webpack dev --debug=5858",
    "dev-no-tests": "electron-webpack dev --debug=5858",
    "compile": "yarn test-compile && yarn test && electron-webpack",
    "compile-no-test": "yarn test-compile && electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist-no-tests": "yarn compile-no-test && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist:dir-no-tests": "yarn dist-no-tests --dir -c.compression=store -c.mac.identity=null",
    "test-compile": "yarn fix && tsc --pretty --noEmit -P tsconfig.json",
    "test-compile-watch": "tsc --watch --pretty --noEmit -P tsconfig.json",
    "test": "yarn fix && yarn test-unit && yarn test-electron",
    "test-unit": "cross-env NODE_ENV=test ts-mocha -p test/ test/unit/**/*.ts",
    "test-unit-watch": "ts-mocha -p test/ test/unit/**/*.ts --watch test/unit/**/*Test.ts --watch-extensions ts",
    "test-unit-cover": "cross-env NODE_ENV=test nyc --reporter=html ts-mocha -p test/ test/unit/**/*.ts",
    "test-electron": "tsc --pretty -P test/tsconfig.json && electron-mocha test/electron/**/*Test.js",
    "test-electron-spectron": "cross-env NODE_ENV=test ts-mocha -p test/ test/electron/**/*spec.ts",
    "test-specific": "cross-env NODE_ENV=test ts-mocha -p test/ test/unit/systemApiTest.ts",
    "test-specific-electron": "tsc --pretty -P test/tsconfig.json && electron-mocha test/electron/**/registryManagerTest.js",
    "kill-ghost-process": "taskkill /F /IM cmd.exe",
    "kill-ghost-process2": "taskkill /F /IM conhost.exe",
    "rebuild-sqlite": "electron-rebuild -f -w sqlite3",
    "resync-typeorm": "ts-node typeorm schema:sync"
  },
